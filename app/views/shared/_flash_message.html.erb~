<% flash.each do |key, value| %>
<% if key == "notice" %>
<!-- <div class="alert alert-info" role="alert"> -->
<% else %>
<div class="alert alert-<%= key %>" role="alert">
  <% end %>
  <button type="button" class="close" data-dismiss="alert" aria-label="Close">
    <span aria-hidden="true">&times;</span>
  </button>
  <%= value %>
</div>
<% end %>
